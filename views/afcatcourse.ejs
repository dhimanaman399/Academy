<body>

    <!--#region Header-->
    <%- include('layouts/header/header')%>
    <!--#endregion-->

    <!--#region Main Page Display-->
    <main>
        <section class="gaArmyOfficerEntry">
            <div class="card-group row">
                <div class="home-left-section card col-sm-9">
                    <div class="card-body">
                        <div class="container">

                            <div class="table-wrap">
                                <h4 class="table-title"> AFCAT Written Exam Preparation

                                </h4>

                                <table class="table table-hover table-bordered cm-tables">
                                    <thead>
                                        <tr class="info">
                                            <th>Subject</th>
                                            <th>Topics</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>General awarness</td>
                                            <td>History, Sports, Geography, Environment, Culture, Current Affairs, Politics , Civics, Basic Sciences, Defence Etc</td>
                                        </tr>
                                        <tr>
                                            <td>General English	</td>
                                            <td>Comprehension, Error Detection, Synonyms, Antonyms, and Testing of Vocabulary, Sentence Completion etc</td>
                                        </tr>
                                        <tr>
                                            <td>Reasoning</td>
                                            <td>Verbal & Nor-Verbal Reasoning</td>
                                        </tr>
                                        <tr>
                                            <td>Mathematics</td>
                                            <td>Decimal Fraction, Simplification, Average, Percentage, Ratio & Proportion, and Simple Interest, Profit & Loss</td>
                                        </tr>
                                        

                                    </tbody>
                                </table>

                            </div>

                            <div class="table-wrap">
                                <h4 class="table-title"> Examination Details

                                </h4>
                                <table class="table table-hover table-bordered cm-tables">
                                    <thead>
                                        <tr class="info">
                                            <th>Subject</th>
                                            <th>Duration</th>
                                            <th>Marks</th>
                                            </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>General Exam</td>
                                            <td>2 hours</td>
                                            <td>300</td>
                                            </tr>
                                            <tr>
                                                <td>Engineering Knowledge Test</td>
                                                <td>45 Mins</td>
                                                <td>150</td>
                                                </tr>

                                    </tbody>
                                </table>

                            </div>


                            <div class="table-wrap">
                              
                                <table class="table table-hover table-bordered cm-tables">
                                    <thead>
                                        <tr class="info">
                                            <th>Paper</th>
                                            <th>Availability Of Forms</th>
                                            <th>Date Of Exam</th>
                                            </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                        <td>AFCAT – 1</td>
                                        <td>Mid December to January</td>
                                        <td>Mid Feb</td>
                                        </tr>
                                        <tr>
                                        <td>AFCAT – 2</td>
                                        <td>Mid June to July</td>
                                        <td>Mid September</td>
                                        </tr>
                                        </tbody>
                                </table>

     
                            </div>

                            <div class="table-wrap">
                                <h4 class="table-title"> Fee structure for AFCAT</h4>
                                <h5>
                                    (Our Batches starts first of every month)
                                </h5>
                                <table class="table table-hover table-bordered cm-tables">
                                    <thead>
                                        <tr class="info">
                                            <th>Course Name</th>
                                            <th>Duration</th>
                                            <th>Batch Time</th>
                                            <th>Fee</th>
                                        </tr>

                                    </thead>
                                    <tbody>

                                        <tr>
                                            <td>Crash Course</td>
                                            <td>30 days</td>
                                            <td>9:30am - 2:30pm</td>
                                            <td>6000/-</td>
                                        </tr>

                                        <tr>
                                            <td>Comprehensive Course</td>
                                            <td>90 days</td>
                                            <td>9:30am - 2:30pm</td>
                                            <td>18000/-</td>
                                        </tr>

                                    </tbody>
                                </table>
                                <p>We Provide personality development and communication skill class to all
                                    candidates.</p>
                                <div class="nots-list">
                                    <p>
                                        Separate <strong>Hostel/PG</strong> Facility for outstation
                                        <strong>Boys/Girls</strong>
                                    </p>
                                    <ul>

                                        <li>
                                            Charge: Rs 7500/month for Boys- <strong>(including breakfast, lunch and
                                                dinner)</strong>
                                        </li>
                                        <li>
                                            Charge: Rs 8000/month for Girls- <strong>(including breakfast, lunch and
                                                dinner)</strong>
                                        </li>
                                    </ul>


                                    <p>
                                        Students of RMS/ Sainik School/Navodaya Vidyalaya/ Kendriya Vidyalaya/ Govt
                                        schools will be given 30% Discounts in 2 Months course
                                    </p>

                                </div>

                                <p>
                                    <strong> NOTE : Students of EWS Category are given 30% Discount.</strong>
                                </p>

                            </div>

                        </div>     
                    </div>
                </div>

                <!--#region Page data -->
                <%- include('layouts/ads/ads')%>
                <!--#endregion-->

